{
  "name": "@hebcal/icalendar",
  "version": "6.3.5",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "ical",
    "icalendar",
    "jewish",
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as iCalendar RFC 2445",
  "type": "module",
  "module": "./dist/icalendar.js",
  "typings": "./dist/icalendar.d.ts",
  "exports": {
    ".": "./dist/icalendar.js"
  },
  "engines": {
    "node": ">= 20"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hebcal/hebcal-icalendar.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-icalendar/issues"
  },
  "homepage": "https://hebcal.github.io/api/icalendar/",
  "dependencies": {
    "@hebcal/core": "^6.0.8",
    "@hebcal/hdate": "^0.21.1",
    "@hebcal/rest-api": "^6.4.0",
    "murmurhash3": "^0.5.0",
    "tslib": "^2.8.1"
  },
  "scripts": {
    "build:rollup": "rollup -c",
    "build:version": "node ./version.cjs package.json src/pkgVersion.ts",
    "build": "npm run build:version && npm run build:rollup",
    "pretest": "npm run build",
    "docs": "typedoc",
    "test": "vitest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@hebcal/learning": "^6.6.1",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/node": "25.0.10",
    "gts": "^7.0.0",
    "rollup": "^4.56.0",
    "typedoc": "^0.28.16",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
