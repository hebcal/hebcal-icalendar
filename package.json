{
  "name": "@hebcal/icalendar",
  "version": "5.1.1",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "ical",
    "icalendar",
    "jewish",
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as iCalendar RFC 2445",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs",
    "types": "./dist/icalendar.d.ts"
  },
  "typings": "dist/icalendar.d.ts",
  "engines": {
    "node": ">= 18.0.0"
  },
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-icalendar.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-icalendar/issues"
  },
  "dependencies": {
    "@hebcal/core": "^5.4.8",
    "@hebcal/rest-api": "^5.1.0",
    "murmurhash3": "^0.5.0",
    "tslib": "^2.6.3"
  },
  "scripts": {
    "build:rollup": "rollup -c",
    "build:version": "node ./version.cjs package.json src/pkgVersion.ts",
    "build": "npm run build:version && npm run build:rollup",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "readme": "npx jsdoc2md dist/index.ts",
    "coverage": "jest --coverage",
    "test": "jest",
    "lint": "gts lint",
    "clean": "gts clean",
    "compile": "tsc",
    "fix": "gts fix"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "@hebcal/learning": "^5.1.1",
    "@rollup/plugin-commonjs": "^26.0.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/jest": "^29.5.12",
    "@types/node": "20.12.7",
    "gts": "^5.3.1",
    "jest": "^29.7.0",
    "jsdoc": "^4.0.3",
    "jsdoc-to-markdown": "^8.0.1",
    "rollup": "^4.18.0",
    "ts-jest": "^29.1.5",
    "typescript": "^5.5.3"
  }
}
