{
  "name": "@hebcal/icalendar",
  "version": "5.0.2",
  "author": "Michael J. Radwin (https://github.com/mjradwin)",
  "keywords": [
    "ical",
    "icalendar",
    "jewish",
    "hebcal"
  ],
  "description": "Jewish holidays and Hebrew calendar as iCalendar RFC 2445",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "type": "module",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.cjs"
  },
  "typings": "icalendar.d.ts",
  "files": [
    "dist",
    "icalendar.d.ts"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/hebcal/hebcal-icalendar.git"
  },
  "bugs": {
    "url": "https://github.com/hebcal/hebcal-icalendar/issues"
  },
  "dependencies": {
    "@hebcal/core": "^5.0.3",
    "@hebcal/rest-api": "^5.0.2",
    "murmurhash3": "^0.5.0"
  },
  "scripts": {
    "build:rollup": "rollup -c",
    "build:version": "node ./version.cjs package.json src/pkgVersion.js",
    "build": "npm run build:version && npm run build:rollup",
    "prepare": "npm run build",
    "pretest": "npm run build",
    "readme": "npx jsdoc2md dist/index.js",
    "test": "ava"
  },
  "license": "BSD-2-Clause",
  "devDependencies": {
    "@hebcal/learning": "^5.0.2",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-json": "^6.1.0",
    "ava": "^6.0.1",
    "eslint": "^8.56.0",
    "eslint-config-google": "^0.14.0",
    "jsdoc": "^4.0.2",
    "jsdoc-to-markdown": "^8.0.0",
    "rollup": "^4.9.1"
  }
}
